// src/Product.php
namespace Entity;
/**
 * @Entity @Table(name="products")
 */
class Product
{
    /**
     * @var int
     * @Id
     * @Column(type="integer") @GeneratedValue 
     */
    protected $id;
    /**
     * @var string
     */
    /**
     *
     * @var type 
     * @Column(type="string")
     */
    protected $name;

    public function getId()
    {
        return $this->id;
    }

    public function getName()
    {
        return $this->name;
    }

    public function setName($name)
    {
        $this->name = $name;
    }
}
